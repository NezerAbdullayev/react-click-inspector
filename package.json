{
  "name": "react-click-inspector",
  "version": "1.0.4",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.tsx --format cjs,esm --dts",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NezerAbdullayev/react-click-inspector.git"
  },
  "homepage": "https://github.com/NezerAbdullayev/react-click-inspector#readme",
  "keywords": [
    "react",
    "inspector",
    "click",
    "debug",
    "developer-tool",
    "devtools",
    "react-component"
  ],
  "bugs": {
    "url": "https://github.com/NezerAbdullayev/react-click-inspector/issues"
  },
  "license": "MIT",
  "author": "Nezer Abdullayev",
  "description": "A lightweight React tool to inspect clicks and element details for debugging UI events.",
  "devDependencies": {
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  }
}
